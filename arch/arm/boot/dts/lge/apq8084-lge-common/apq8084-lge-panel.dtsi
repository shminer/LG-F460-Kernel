	dsi_lgd_sic_1440_vid0: qcom,mdss_dsi_lgit_sic_mipi0_video {
		compatible = "qcom,mdss-dsi-panel";
		qcom,mdss-dsi-panel-name = "LGD 1440p SIC video mode dsi primary panel";
		qcom,mdss-dsi-panel-controller = <&mdss_dsi0>;
