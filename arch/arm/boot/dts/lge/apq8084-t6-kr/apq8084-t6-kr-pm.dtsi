&pma8084_gpios {
	gpio@c000 { /* GPIO 1 */
		status = "disable";
	};

	gpio@c100 { /* GPIO 2 */
		status = "ok";           /* KYPD_VOL_UP_N */
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
	};

	gpio@c200 { /* GPIO 3 */
		status = "ok";           /* DIVCLK1_REQ */
	};

	gpio@c300 { /* GPIO 4 */
		status = "ok";           /* BT_RESET_N */
		qcom,mode = <1>;         /* Digital output*/
		qcom,pull = <4>;         /* Pulldown 10uA */
		qcom,vin-sel = <2>;      /* VIN2 */
		qcom,src-sel = <0>;      /* GPIO */
		qcom,invert = <0>;       /* Invert */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	gpio@c400 { /* GPIO 5 */
		status = "ok";           /* KYPD_VOL_DOWN_N */
		qcom,mode = <0>;
		qcom,pull = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
	};

	gpio@c500 { /* GPIO 6 */
		status = "disable";
	};

	gpio@c600 { /* GPIO 7 */
		status = "ok";           /* RED_LED_DRV */
	};

	gpio@c700 { /* GPIO 8 */
		status = "ok";           /* GREEN_LED_DRV */
	};

	gpio@c800 { /* GPIO 9 */
		status = "ok";           /* BLUE_LED_DRV */
	};

	gpio@c900 { /* GPIO 10 */
		status = "ok";           /* NFC_CLK_REQ */
		qcom,mode = <0>;         /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <5>;         /* QPNP_PIN_PULL_NO */
		qcom,vin-sel = <2>;      /* QPNP_PIN_VIN2 */
		qcom,src-sel = <2>;      /* QPNP_PIN_SEL_FUNC_1 */
		qcom,master-en = <1>;
	};

	gpio@ca00 { /* GPIO 11 */
		status = "ok";           /* SLIMPORT_CBL_DET */
		qcom,mode = <0>;         /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <4>;         /* QPNP_PIN_PULL_DN */
		qcom,vin-sel = <2>;      /* QPNP_PIN_VIN2 */
		qcom,src-sel = <0>;      /* QPNP_PIN_SEL_FUNC_1 */
		qcom,master-en = <1>;
	};

	gpio@cb00 { /* GPIO 12 */
		status = "ok";           /* EXT_CHG_STAT */
		qcom,mode = <0>;         /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <3>;         /* QPNP_PIN_GPIO_PULL_UP_1P5_30 */
		qcom,vin-sel = <2>;      /* QPNP_PIN_VIN2 */
		qcom,src-sel = <0>;      /* QPNP_PIN_SEL_FUNC_CONSTANT */
		qcom,master-en = <1>;
	};

	gpio@cc00 { /* GPIO 13 */
		status = "ok";           /* ANX_PDWN_CTL */
		qcom,mode = <1>;         /* Digital output */
		qcom,pull = <5>;         /* QPNP_PIN_PULL_NO */
		qcom,output-type = <0>;  /* CMOS logic */
		qcom,invert = <0>;       /* Output low initially */
		qcom,vin-sel = <2>;      /* VIN 2 */
		qcom,src-sel = <2>;      /* Function 1 */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	gpio@cd00 { /* GPIO 14 */
		status = "ok";           /* EXT_CHG_INT */
		qcom,mode = <0>;         /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <3>;         /* QPNP_PIN_GPIO_PULL_UP_1P5_30 */
		qcom,vin-sel = <2>;      /* QPNP_PIN_VIN2 */
		qcom,src-sel = <0>;      /* QPNP_PIN_SEL_FUNC_CONSTANT */
		qcom,master-en = <1>;
	};

	gpio@ce00 { /* GPIO 15 */
		status = "ok";           /* CODEC_MCLK */
		qcom,mode = <1>;         /* Digital output */
		qcom,output-type = <0>;  /* CMOS logic */
		qcom,invert = <0>;       /* Output low initially */
		qcom,vin-sel = <2>;      /* VIN 2 */
		qcom,src-sel = <2>;      /* Function 1 */
		qcom,out-strength = <3>; /* High */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	gpio@cf00 { /* GPIO 16 */
		status = "ok";           /* WIFI_SLEEP_CLK, T6 BT Bringup kyungrin.seo */
		qcom,mode = <1>;
		qcom,output-type = <0>;
		qcom,pull = <5>;
		qcom,vin-sel = <2>;
		qcom,out-strength = <3>;
		qcom,src-sel = <3>;
		qcom,master-en = <1>;

	};

	gpio@d000 { /* GPIO 17 */
		status = "ok";           /* SLIMPORT_RESET_N */
		qcom,mode = <1>;         /* Digital output */
		qcom,output-type = <0>;  /* CMOS logic */
		qcom,pull = <4>;         /* QPNP_PIN_PULL_DN */
		qcom,invert = <0>;       /* Output low initially */
		qcom,vin-sel = <2>;      /* VIN 2 */
		qcom,src-sel = <2>;      /* Function 1 */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	gpio@d100 { /* GPIO 18 */
		status = "ok";           /* EXT_CHG_EN */
		qcom,mode = <1>;         /* Digital output*/
		qcom,pull = <4>;         /* Pulldown 10uA */
		qcom,vin-sel = <2>;      /* VIN2 */
		qcom,src-sel = <0>;      /* GPIO */
		qcom,invert = <0>;       /* Invert */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	gpio@d200 { /* GPIO 19 */
		status = "ok";           /* BOOST_BYP_BYP_N */
		qcom,mode = <0>;         /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <4>;         /* QPNP_PIN_PULL_DN */
		qcom,vin-sel = <2>;      /* VIN2 */
		qcom,src-sel = <0>;      /* GPIO */
		qcom,invert = <0>;       /* Invert */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	gpio@d300 { /* GPIO 20 */
		status = "ok";           /* WIFI_SLEEP_CLK */
		qcom,mode = <1>;         /* Digital output */
		qcom,output-type = <0>;  /* CMOS logic */
		qcom,invert = <0>;       /* Do not invert the output */
		qcom,vin-sel = <0>;      /* VPH_PWR */
		qcom,src-sel = <0>;      /* GPIO */
		qcom,out-strength = <1>; /* High drive strength */
		qcom,master-en = <0>;    /* Enable GPIO */          /* WIFI_SLEEP_CLK, T6 WiFi Bringup. Set master-en = <0>;  */
	};

	gpio@d400 { /* GPIO 21 */
		status = "ok";           /* EXT_CHG_PG */
		qcom,mode = <0>;         /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <3>;         /* QPNP_PIN_GPIO_PULL_UP_1P5_30 */
		qcom,vin-sel = <2>;      /* QPNP_PIN_VIN2 */
		qcom,src-sel = <0>;      /* QPNP_PIN_SEL_FUNC_CONSTANT */
		qcom,master-en = <1>;
	};

	gpio@d500 { /* GPIO 22 */
		status = "ok";           /* BATT_UIM_ALARM */
	};
};

&pma8084_mpps {
	mpp@a000 { /* MPP 1 */
	};

	mpp@a100 { /* MPP 2 */
		qcom,mode = <1>;         /* Digital output */
		qcom,output-type = <0>;  /* CMOS logic */
		qcom,vin-sel = <2>;      /* VIN 2 */
		qcom,src-sel = <0>;      /* Constant */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	mpp@a200 { /* MPP 3 */
	};

	mpp@a300 { /* MPP 4 */
		qcom,mode = <1>;         /* Digital output */
		qcom,output-type = <0>;  /* CMOS logic */
		qcom,vin-sel = <2>;      /* VIN 2 */
		qcom,src-sel = <0>;      /* Constant */
		qcom,master-en = <1>;    /* Enable GPIO */
	};

	mpp@a400 { /* MPP 5 */
		qcom,mode = <4>;		/* QPNP_PIN_MODE_AIN */
		qcom,invert = <1>;		/* Enable MPP */
		qcom,ain-route = <0>;	/* QPNP_PIN_AIN_AMUX_CH5 */
		qcom,master-en = <1>;
		qcom,src-sel = <0>;	/* QPNP_PIN_SEL_FUNC_CONSTANT */
	};

	mpp@a500 { /* MPP 6 */
	};

	mpp@a600 { /* MPP 7 */
		qcom,mode = <4>;
		qcom,invert = <0>;
		qcom,output-type = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		qcom,ain-route = <2>;
		qcom,master-en = <1>;
	};

	mpp@a700 { /* MPP 8 */
	};
};

&rpm_bus {
	rpm-regulator-ldoa9 {
		status = "okay";
		pma8084_l9: regulator-l9 {
			regulator-always-on;
			status = "okay";
		};
	};
	rpm-regulator-ldoa10 {
		status = "okay";
		pma8084_l10: regulator-l10 {
			qcom,init-enable = <0>;
			status = "okay";
		};
	};
	rpm-regulator-ldoa14 {
		status = "okay";
		pma8084_l14: regulator-l14 {
			qcom,init-enable = <0>;
			status = "okay";
		};
	};
	rpm-regulator-ldoa16 {
		status = "okay";
		pma8084_l16: regulator-l16 {
			qcom,init-enable = <0>;
			status = "okay";
		};
	};
	rpm-regulator-ldoa19 {
		pma8084_l19: regulator-l19 {
			regulator-min-microvolt = <3050000>;
			regulator-max-microvolt = <3050000>;
			qcom,init-voltage = <3050000>;
			status = "okay";
		};
	};
	rpm-regulator-ldoa23 {
		status = "okay";
		pma8084_l23: regulator-l23 {
			qcom,init-enable = <0>;
			status = "okay";
		};
	};
	rpm-regulator-ldoa26 {
		status = "okay";
		pma8084_l26: regulator-l26 {
			qcom,init-enable = <0>;
			status = "okay";
		};
	};
};
